{
	"name": "live2eat",
	"productName": "Live To Eat",
	"version": "12.05.2020",
	"description": "recipe storage app",
	"license": "MIT",
	"repository": "mohammadmoustafa/live2eat",
	"author": {
		"name": "Mohamed Moustafa",
		"url": "https://mohammadmoustafa.tech"
	},
	"main": "dist/main.js",
	"scripts": {
		"start": "gulp start",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --linux --windows",
		"release": "np",
		"build": "gulp build"
	},
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-react-app": "^3.1.2",
		"electron-context-menu": "^0.15.0",
		"electron-debug": "^3.0.0",
		"electron-is-dev": "^1.2.0",
		"electron-serve": "^1.0.0",
		"electron-store": "^5.1.0",
		"electron-timber": "^0.5.1",
		"electron-unhandled": "^3.0.0",
		"electron-updater": "^4.0.6",
		"electron-util": "^0.13.0",
		"fsevents": "^2.1.3",
		"react-router-dom": "^5.2.0",
		"url": "^0.11.0",
		"xel": "0.0.220"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		]
	},
	"np": {
		"publish": false,
		"releaseDraft": false
	},
	"build": {
		"appId": "com.mohammadmoustafa.live2eat",
		"directories": {
			"app": "dist"
		},
		"mac": {
			"category": "public.cooking",
			"darkModeSupport": true
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			],
			"category": "Cooking"
		}
	},
	"_moduleAliases": {
		"@components": "./src/components"
	}
}
